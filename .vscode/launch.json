{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch romp (german test)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/romp/romp",
            "args": [
                "--output",
                "${workspaceFolder}/romp/tests/german.test.cpp",
                "${workspaceFolder}/romp/tests/german.m"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake: build romp"
        },
        {
            "name": "(gdb) Launch romp (german test) [no-build]",
            "type": "cppdbg",
            "request": "launch",
