        {
            "name": "C/C++: romp-rw g++ build and debug active file (single-walk test)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [
                "-sw", "-s", "test-seed"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
